{% extends 'base.html.twig' %}

{% block content %}
    <div class="container">
        <h2>Modifier un adhérent Particulier</h2>
        {{ form_start(form) }}
        {{ form_row(form.email, { 'label': 'Email'}) }}
    {{ form_row(form.lastName, { 'label': 'Nom' }) }}
    {{ form_row(form.firstName, { 'label': 'Prénom' }) }}
    {{ form_row(form.birthdate, { 'label': 'Date de naissance' }) }}
{{ form_row(form.save, { 'label': 'Modifier le Particulier'}) }}
{{ form_end(form) }}
    </div>
{% endblock %}